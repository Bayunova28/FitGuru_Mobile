<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.uas.fitguru">

    <application
        android:allowBackup="true"
        android:icon="@drawable/fat_new2"
        android:roundIcon="@drawable/fat_new2"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".WorkoutDetailActivity"></activity>
        <activity android:name=".EditFood" />
        <activity android:name=".AddFood" />
        <activity android:name=".RatingFragment" />
        <activity android:name=".SignUp" />
        <activity android:name=".Login" />
        <activity android:name=".MainActivity" />
        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>